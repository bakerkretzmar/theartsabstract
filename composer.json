{
    "name": "bakerkretzmar/theartsabstract",
    "type": "project",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.4",
        "oscarotero/env": "^1.2.0",
        "roots/soil": "^3.8",
        "roots/wordpress": "5.2.2",
        "roots/wp-config": "1.0.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "vlucas/phpdotenv": "^3.4.0",
        "wpackagist-plugin/8tracks-shortcode": "^1.35",
        "wpackagist-plugin/advanced-custom-fields": "^5.8",
        "wpackagist-plugin/co-authors-plus": "^3.4",
        "wpackagist-plugin/wps-hide-login": "1.5.2.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4.2",
        "roave/security-advisories": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin", "wpackagist-plugin/advanced-custom-fields", "wpackagist-plugin/redis-cache", "wpackagist-plugin/wps-hide-login", "roots/soil"],
            "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
            "web/app/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {}
}
